        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".p1_intro" android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".p2_start" android:screenOrientation="landscape"/>
        <activity android:name=".p3_dificultad" android:screenOrientation="landscape"/>
        <activity android:name=".p_preguntas" android:screenOrientation="landscape"/>
    </application>

</manifest>